{{#if profiles}}
<ul class="profiles-list">
    {{#each profiles}}
    <li id="profile-{{id}}" class="profiles-list__elem">
        <a href="#profile/{{id}}" class="profiles-elem__link">
        <section class="details details_hor-center">
            <p class="details__elem">{{firstName}}</p>
            <p class="details__elem">{{age}}</p>
        </section>
        </a>
    </li>
    {{/each}}
</ul>
<div class="profiles-list__paginator">
    <div class="paginator">
        <span class="paginator__left">
        <a class="paginator__arrow_style" href="#profile/{{prevId}}"> &larr;</a>
        </span>
        <span class="paginator__page-num">{{selected}}</span>
        <span class="paginator__right">
        <a class="paginator__arrow_style" href="#profile/{{nextId}}">&rarr;</a>
        </span>
    </div>
</div>
{{else}}
    <p>There no profiles</p>
{{/if}}
